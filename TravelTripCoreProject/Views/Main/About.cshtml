@using TravelTripCoreProject.Models.Classes
@model List<About>
@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var item in Model)
            {
                <img src="@item.PhotoUrl" alt="" />
                <p>@item.Description</p>

            }

        </div>
    </div>
</div>


